{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console" : "off",
        "indent" : "off",
        "linebreak-style" : "off",
        "eol-last" : "off",
        "no-trailing-spaces" : "off"
    }
}
